{
    "name": "raichev/twig-turbo-bundle",
    "description": "Opinionated extension for Symfony UX Turbo",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Raichev\\TwigTurboBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raichev\\TwigTurboBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dimitar Raichev",
            "email": "dimitar@raichev.eu"
        }
    ],
    "require": {
        "php": "^7.4|^8",
        "symfony/config": "^6|^7",
        "symfony/dependency-injection": "^6|^7",
        "symfony/twig-bundle": "^6|^7"
    },
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "symfony/phpunit-bridge": "^6|^7"
    }
}
